version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:cdaf824d6ff1c4ab2e8bc5c695307e0393cb1fb8
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
