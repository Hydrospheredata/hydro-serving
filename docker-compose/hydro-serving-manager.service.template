version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:1cf4a434c7f960aa058494148fbcc9ef2b512fcc
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
