version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:6f2b3d806606bf36cb6070c455c16f5d1154bf00
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
