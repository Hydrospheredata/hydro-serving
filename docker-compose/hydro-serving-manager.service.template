version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:d24efab3a5618e52c5e9e0769ed82092f0bf7de6
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
