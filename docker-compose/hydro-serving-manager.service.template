version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:8a6ca8968b1c8565648e705b83bae3c917945e05
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
