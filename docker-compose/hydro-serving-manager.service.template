version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:6f3ede6e08c58710cb96960b01eefb74b50cfda7
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
