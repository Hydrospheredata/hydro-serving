version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:c7583bb57077c4a08da522f146418f6d8f2fedf1
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
