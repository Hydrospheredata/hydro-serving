version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:4b677f430c0a5a1bf5e99a9f5b9deda69086d356
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
