version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:6da185e856431e75b998a6956f58d700394b7a09
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
