version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:c2739c675ec5d159c210d01c33fd301d85993d7c
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      CUSTOM_CONFIG: /app/config/dc.conf
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
