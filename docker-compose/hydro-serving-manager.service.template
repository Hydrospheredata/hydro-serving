version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:54956bb7bac0947e9bb2b42c26d42249085f2208
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
